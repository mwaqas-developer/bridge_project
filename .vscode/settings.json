{
   "editor.tabSize": 3,
   "editor.insertSpaces": true,
   "editor.detectIndentation": false,
   "editor.formatOnSave": true,
   "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
   },
   "[javascript]": {
      "editor.tabSize": 3
   },
   "[javascriptreact]": {
      "editor.tabSize": 3
   },
   "[typescript]": {
      "editor.tabSize": 3
   },
   "[typescriptreact]": {
      "editor.tabSize": 3
   },
   "[json]": {
      "editor.tabSize": 3
   },
   "[css]": {
      "editor.tabSize": 3
   },
   "[html]": {
      "editor.tabSize": 3
   },
   "codium.codeCompletion.enable": false,
   "CodeGPT.apiKey": "CodeGPT Plus Beta"
}